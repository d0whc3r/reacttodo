{
  "name": "reacttodo",
  "version": "1.0.0",
  "description": "simple react todo app",
  "main": "index.js",
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.15.3",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.26.1",
    "deep-freeze-strict": "^1.1.1",
    "expect": "^1.20.2",
    "express": "^4.14.0",
    "firebase": "^3.6.4",
    "foundation-sites": "^6.3.0",
    "jquery": "^3.1.1",
    "moment": "^2.17.1",
    "node-env-file": "^0.1.8",
    "node-sass": "^4.1.1",
    "node-uuid": "^1.4.7",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-redux-multilingual": "^1.0.2",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-mock-store": "^1.2.1",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.1.1",
    "script-loader": "^0.7.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0"
  },
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron": "^1.4.13",
    "electron-debug": "^1.1.0",
    "electron-packager": "^8.4.0",
    "electron-rebuild": "^1.4.0",
    "electron-reload": "^1.1.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.1",
    "mocha": "^3.2.0"
  },
  "electronOut": "dist-electron",
  "electronPath": "./public",
  "electronVersion": "1.4.13",
  "scripts": {
    "test": "NODE_ENV=test karma start",
    "build": "NODE_ENV=development webpack --progress",
    "build-prod": "NODE_ENV=production webpack --progress --colors",
    "packelectron-linux": "npm run build-prod && npm run pack",
    "start": "npm run build && node server.js",
    "electron": "npm run build && npm run electron-run",
    "electron-run": "NODE_ENV=development electron $npm_package_electonPath",
    "oldpostinstall": "webpack --config ./webpack.config.js --progress --colors --display-error-details --display-cached",
    "pack": "electron-packager $npm_package_electronPath $npm_package_name --app-version=$npm_package_version --out=$npm_package_electronOut --prune --asar --platform=linux --arch=x64 --version=$npm_package_electronVersion --overwrite",
    "pack-win-linux": "electron-packager $npm_package_electronPath $npm_package_name --app-version=$npm_package_version --out=$npm_package_electronOut --prune --asar --platform=win32 --arch=x64 --version=$npm_package_electronVersion --version-string.OriginalFilename=$npm_package_name --version-string.InternalName=$npm_package_name --version-string.ProductName=$npm_package_name --version-string.ProductVersion=$npm_package_version --icon=./app/favicon.ico --overwrite",
    "pack-win": "electron-packager $npm_package_electronPath %npm_package_name% --app-version=%npm_package_version% --out=%npm_package_electronOut% --prune --asar --platform=win32 --arch=x64 --version=%npm_package_electronVersion% --version-string.OriginalFilename=%npm_package_name% --version-string.InternalName=%npm_package_name% --version-string.ProductName=%npm_package_name% --version-string.ProductVersion=%npm_package_version% --icon=./app/favicon.ico --overwrite"
  }
}

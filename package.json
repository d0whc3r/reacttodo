{
  "name": "reacttodo",
  "version": "1.0.0",
  "description": "simple react todo app",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test karma start",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./app/main.development",
    "oldpostinstall": "webpack --config ./webpack.config.js --progress --colors --display-error-details --display-cached",
    "lint": "eslint --ignore-path .gitignore --format=node_modules/eslint-formatter-pretty app test *.jsx",
    "lint-fix": "npm run lint -- --fix",
    "flow-typed-install": "flow-typed install",
    "postinstall": "concurrently \"npm run flow-typed-install\" \"install-app-deps\" \"node node_modules/fbjs-scripts/node/check-dev-engines.js package.json\"",
    "package": "npm run build && build --publish never",
    "package-win": "npm run build && build --win --x64",
    "package-linux": "npm run build && build --linux",
    "package-all": "npm run build && build -mwl",
    "cleanup": "mop -v"
  },
  "build": {
    "productName": "ElectronReact",
    "path": "public",
    "appId": "org.develar.ElectronReact",
    "category": "public.app-category.tools",
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "files": [
      "dist/",
      "node_modules/",
      "app.html",
      "main.js",
      "main.js.map",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    }
  },
  "directories": {
    "buildResources": "resources",
    "output": "release"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "devEngines": {
    "node": ">=6.x",
    "npm": ">=3.x"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.15.3",
    "deep-freeze-strict": "^1.1.1",
    "electron-debug": "^1.1.0",
    "expect": "^1.20.2",
    "firebase": "^3.6.4",
    "font-awesome": "^4.7.0",
    "foundation-sites": "^6.3.0",
    "jquery": "^3.1.1",
    "moment": "^2.17.1",
    "node-env-file": "^0.1.8",
    "node-sass": "^4.1.1",
    "node-uuid": "^1.4.7",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-redux-multilingual": "^1.0.2",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-mock-store": "^1.2.1",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.1.1",
    "script-loader": "^0.7.0",
    "source-map-support": "^0.4.7"
  },
  "devDependencies": {
    "asar": "^0.12.3",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-tcomb": "^0.3.24",
    "babel-plugin-webpack-loaders": "^0.8.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.1.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "babili-webpack-plugin": "0.0.7",
    "concurrently": "^3.1.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "devtron": "^1.4.0",
    "electron": "^1.4.13",
    "electron-builder": "^10.9.3",
    "electron-devtools-installer": "^2.0.1",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-import-resolver-webpack": "^0.8.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-flowtype-errors": "^2.0.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.8.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fbjs-scripts": "^0.7.1",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.37.4",
    "flow-typed": "^2.0.0",
    "html-webpack-plugin": "^2.24.1",
    "jsdom": "^9.9.1",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.1",
    "minimist": "^1.2.0",
    "mocha": "^3.2.0",
    "react-addons-test-utils": "^15.4.1",
    "redux-logger": "^2.7.4",
    "spectron": "^3.4.1",
    "style-loader": "^0.13.1",
    "tcomb": "^3.2.15",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.14.0",
    "webpack-merge": "^2.0.0",
    "webpack-validator": "^2.3.0"
  }
}
